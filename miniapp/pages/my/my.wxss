.infor_list::before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1rpx;
   border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;
}
.infor_list::after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
   border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;
}
